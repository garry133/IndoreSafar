<%- include("../partials/header") %>
    <br>
    <div style="background-color: black; color: aliceblue;" class="jumbotron">
        <div class="container">
            <!-- <form style="border: thin whitesmoke solid; margin: 20px;padding: 20px;" action="/campgrounds"
                method="POST"> -->
            <form style="border: thin whitesmoke solid; margin: 20px;padding: 20px;" action="/places" method="POST">
                <h1>Create a new Places!!</h1>
                <div class="form-group">
                    <label for="inlineFormInputGroup">Name</label>
                    <input type="text" name="name" class="form-control" id="inlineFormInputGroup" placeholder="name">
                </div>
                <div class="form-group">
                    <label for="inlineFormInputGroup">Image url</label>
                    <input class="form-control" type="text" name="image" id="inlineFormInputGroup"
                        placeholder="image url">
                </div>
                <div class="form-group">
                    <label for="inlineFormInputGroup">Description</label>
                    <input class="form-control" type="text" name="description" id="inlineFormInputGroup"
                        placeholder="description">
                </div>
                <button type="submit" class="btn btn-primary">Submit!</button>
            </form>
            <!-- <a href="/campgrounds">Go back</a> -->
            <a href="/places">Go back</a>
        </div>
    </div>

    <br>
